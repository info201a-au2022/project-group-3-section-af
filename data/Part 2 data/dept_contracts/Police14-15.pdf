<div id="COVID_TEMP-messages" style="display:none;">
<div class="item">
	<div class="title">Housing and Utilities Emergency Relief Funds</div>
	<div class="description">Emergency relief funds are now available for those needing financial assistance for housing and utilities due to the impact of COVID-19.</div>
	<div class="link"><p class="view-page-link"><a href="/files/sharedassets/public/transportation-amp-utilities/housing-utility-assist.pdf"  target="_self">More Information</a></p></div>
	<div class="type">Announcement</div>
	<div class="conditions"><a href="/Water-and-Wastewater-Customer-Service-and-Billing"  target="_self" >contains</a></div>	
</div>
</div>
<script>
	$('#COVID_TEMP-messages').children('.item').each(function(){
		var $message = $(this);
		var isMatch = false;
		$message.find('.conditions a').each(function(){
			var $a = $(this);
			var mode = $a.text().split(' ')[0].toLowerCase();
			var value = $a.attr('href').replace(/^https?:\/\//g, '');
			
			switch(mode)
			{
				case 'startswith':
					if(document.location.pathname.toLowerCase().startsWith(value.toLowerCase()))
					{
						isMatch = true;
						return false;//stop jquery each
					}
					break;
					
				case 'contains':
					if(document.location.pathname.toLowerCase().indexOf(value.toLowerCase()) !== -1)
					{
						isMatch = true;
						return false;//stop jquery each
					}
					break;
					
				case 'regex':
					var re = new RegExp(value);
					if(new RegExp(value).test(document.location.pathname))
					{
						isMatch = true;
						return false;//stop jquery each
					}
					break;
					
				default:
					if(document.location.pathname.toLowerCase() === value.toLowerCase())
					{
						isMatch = true;
						return false;//stop jquery each
					}
					break;
			}
		});
		if(isMatch)
		{
			var $main = $('#main-content');
			if($main.length === 0) return false;
			var $h1 = $main.find('h1').first();
			
			var title = $message.find('.title').text().trim();
			var type = $message.find('.type').text().trim();
			var description = $message.find('.description').html();
			var link = $message.find('.link').html();
			
			var $content = $('<section>').addClass('obj-venue');//class is a hack to inherit styling
			var $sideBox = $('<div>').addClass('side-box message-box message-type-'+type).appendTo($content);
			
			if(title !== '')
			{
				$('<h2>').addClass('side-box-title').text(title).appendTo($sideBox);
			}
			
			var descriptionBox = ''
			
			if(description.trim() !== '')
			{
				//If the description contains no html tags, wrap the text in p tags
				var noBrDesc = description.split('<br>').join('\r\n');
				if (!/<[^>]+>/.test(noBrDesc)) {
					description = '<p>' + noBrDesc.replace(/(\r\n|\r|\n){2}((\r\n|\r|\n)+)?/g, '$2</p><p>') + '</p>';
					description = description.replace(/(\r\n|\r|\n)/g, '<br>');
					description = description.replace('</p><p></p>', '</p>');					
				}
				descriptionBox += description;
			}
			
			var hasLink = false;
			if(link.trim() !== '')
			{
				hasLink = true;
				descriptionBox += link;
			}
			
			var $descriptionBox = null;
			if(descriptionBox.trim() !== '')
			{
				$descriptionBox = $(descriptionBox);
				$('<div>').addClass('side-box-content').html(descriptionBox).appendTo($sideBox);
			}
			
			if($h1.length === 1)
			{
				$content.insertAfter($h1);
			} 
			else
			{
				$content.appendTo($main);
			}
			
			if(hasLink)
			{
				var color = ($content.find('p').first() || $content.find('.side-box-content').first()).css('color');
				$content.find('a').css('color', color);
			}
			
			 return false;
		}
	});
</script>






